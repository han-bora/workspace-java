package kr.co.himedia;

public class XTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int i, j;
		for(i = 1; i<6; i++) {
			for(j=1; j<=i; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
		
		int x, y;
		for(x=6; x>1; x++) {
			for(y=6; y>=1; y++) {
				System.out.print("*");
			}
			System.out.println();
		}

	}

}
