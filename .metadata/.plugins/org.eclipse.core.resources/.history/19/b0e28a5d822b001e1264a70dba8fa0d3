package kr.co.himedia.list;

public class MemberArrayListTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		MemberArrayList memberArrayList = new MemberArrayList();
		
		Member memberL = new Member(2023, "이순신");
		Member memberK = new Member(2024, "강감찬");
		Member memberP = new Member(2025, "박혁거세");
		Member memberS = new Member(2026, "신사임당");
		
		memberArrayList.addMember(memberL);
		memberArrayList.addMember(memberK);
		memberArrayList.addMember(memberP);
		memberArrayList.addMember(memberS);
		
		memberArrayList.showAllMembers();

	}

}
